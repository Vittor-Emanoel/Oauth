generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String @id @default(uuid()) @db.Uuid
  first_name String @map("first_name")
  last_name  String @map("last_name")
  avatar_url String @map("avatar_url")
  email      String @unique
  googleId   String

  @@map("users")
}
